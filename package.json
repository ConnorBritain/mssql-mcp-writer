{
  "name": "@connorbritain/mssql-mcp-writer",
  "version": "0.2.0",
  "description": "MCP server for Microsoft SQL Server with read and data operations - no DDL/schema changes",
  "keywords": [
    "mcp",
    "sql-server",
    "mssql",
    "database",
    "ai",
    "claude",
    "windsurf",
    "model-context-protocol"
  ],
  "author": "Connor England <connor.r.england@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/ConnorBritain/mssql-mcp-writer.git"
  },
  "homepage": "https://github.com/ConnorBritain/mssql-mcp-writer",
  "bugs": {
    "url": "https://github.com/ConnorBritain/mssql-mcp-writer/issues"
  },
  "main": "dist/index.js",
  "bin": {
    "mssql-mcp-writer": "dist/index.js"
  },
  "scripts": {
